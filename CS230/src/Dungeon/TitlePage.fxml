<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="Dungeon.Controller"
            prefHeight="500" prefWidth="600.0" style="-fx-background-image: url(Dungeon/gameTitle.jpg);">
    <bottom>
    <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
        <children>
            <Button fx:id="startGame" text="Run" onAction="#launchLogin" maxWidth="205" maxHeight="64" HBox.hgrow="ALWAYS" style="-fx-background-image: url(starStart.jpg);"/>
        </children>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
    </HBox>
</bottom>

</BorderPane>